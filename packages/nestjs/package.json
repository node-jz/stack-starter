{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start-background": "nest start --config nest-cli-background.json",
    "start-background:dev": "nest start --watch  --config nest-cli-background.json",
    "start-background:debug": "nest start --debug --config nest-cli-background.json --watch",
    "start-background:prod": "node dist/main-background",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "ts-node package_scripts/migrationGenerate.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/ormdatasource.ts",
    "migration:create": "npm run typeorm migration:create -- -d src/ormdatasource.ts src/db/migrations/",
    "migration:revert": "npm run typeorm migration:revert -- -d src/ormdatasource.ts"
  },
  "dependencies": {
    "@dataui/crud": "^5.3.1",
    "@dataui/crud-request": "^5.3.1",
    "@dataui/crud-typeorm": "^5.3.1",
    "@getbrevo/brevo": "2.0.0-beta.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/testing": "^9.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.5",
    "@types/jest": "29.5.0",
    "@types/luxon": "^3.3.2",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "bull": "^4.12.2",
    "dotenv": "^16.3.1",
    "jest": "29.5.0",
    "langchain": "^0.0.168",
    "luxon": "^3.4.3",
    "openai": "^4.29.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "playwright": "1.34.3",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typeorm": "^0.3.17",
    "typescript": "^4.7.4",
    "uuid": "^9.0.1",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
